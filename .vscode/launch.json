{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug (Chrome)",
      "program": "packages/app/lib/main.dart",
      "request": "launch",
      "type": "dart",
      "deviceId": "${input:device}",
      "args": ["--debug"]
    }
  ],
  "inputs": [
    {
      "id": "device",
      "type": "pickString",
      "description": "Select your Device",
      "options": ["chrome"],
      "default": "chrome"
    }
  ]
}
